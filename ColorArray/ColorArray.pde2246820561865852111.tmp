color [] colorArray;

void setup() {

  size(1200, 1000);
  colorArray = new color[5];
  colorArray[0] = color(0, 119, 204,10);
  colorArray[1] = color(0, 45, 76,10);
  colorArray[2] = color(66, 152, 212,10);
  colorArray[3] = color(105, 128, 145,10);
  colorArray[4] = color(0, 90, 153,10);
}

void draw() {

  noStroke();
  for (int i = 0; i < 105; i++) {
    int mod = i % 5;
    fill(colorArray[mod]);
    pushMatrix();
    translate(600,500);
    rotate(i);
    rect(0,0, 100, 2000);
    popMatrix();
  }
}